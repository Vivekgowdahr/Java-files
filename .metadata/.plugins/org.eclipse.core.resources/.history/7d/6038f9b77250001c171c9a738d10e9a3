package com.TY.array;

import java.util.Iterator;
import java.util.Scanner;

public class Swapping {
	public static void main(String[] args) {
		int []a= {7,8,9,10,11,12};
		a[0]=a[0]+1;
		a[1]=a[1]-1;
		a[2]=a[2]+1;
		a[3]=a[3]-1;
		a[4]=a[4]+1;
		a[5]=a[5]-1;
		for (int i = 0; i < a.length; i++) {
			System.out.println(a[i]);
		}
	}
}
