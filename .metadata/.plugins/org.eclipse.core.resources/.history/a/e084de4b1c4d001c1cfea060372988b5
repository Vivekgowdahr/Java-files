package com.TY.arraypractise;

import java.util.Iterator;

public class Sorting 
{
public static void main(String[] args) 
{
	int c;
	int[] a= {3,18,17,45,99,7,1,10,8,12};
	for (int i = 0; i < a.length; i++)
	{
		for (int j = 0; j < a.length; j++)
		{
			if (a[i]<a[j]) 
			{
			c=a[1];
			a[i]=a[j];
			a[j]=c;
			}
		}
	}
}
}
